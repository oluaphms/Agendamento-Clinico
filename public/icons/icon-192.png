  <!-- Anéis do calendário (retângulos no topo) -->
  <rect x="14" y="8" width="4" height="8" rx="2" fill="#2196F3"/>
  <rect x="30" y="8" width="4" height="8" rx="2" fill="#2196F3"/>
  
  <!-- Estetoscópio verde -->
  <!-- Tubo principal (lateral esquerda) -->
  <path d="M8 20V40" stroke="#4DD0A4" stroke-width="2" stroke-linecap="round"/>
  
  <!-- Hastes auriculares (curvas duplas elegantes saindo do topo) -->
