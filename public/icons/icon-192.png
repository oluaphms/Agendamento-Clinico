<svg width="64" height="64" viewBox="0 0 64 64" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Fundo branco -->
  <rect width="64" height="64" fill="white"/>
  
  <!-- Calendário azul com cantos arredondados -->
